  "scripts": {
    "assets": "mkdir ./public/assets && concurrently -r \"npm run fonts\" \"npm run images\" \"npm run i18n\"",
    "build:client": "concurrently -r \"npm run clean\" \"npm run bundle\"",
    "build": " npm run build:client",
    "bundle": "npm run less && npm run pack",
    "clean": "rimraf public/* && npm run assets",
    "fonts": "concurrently -r \"cp -R node_modules/font-awesome/fonts ./public\" \"cp -R node_modules/@mdi/font/fonts ./public\"",
    "images": "cp -R client/src/assets/images ./public/assets/images",
    "i18n": "cp -R client/src/assets/i18n ./public/assets/i18n",
    "less": "mkdirp public/dist && lessc ./client/src/app/styles/main.less > ./public/dist/app.css --clean-css=\"--s0 --advanced\"",
    "start": " npm run build && node ./lib/app.js",
    "pack": "webpack --config webpack.config.dev.js",
    "test": "karma start ./karma.conf.js",
    "tslint": "npm run tslint:client",
    "tslint:client": "\"client/src/**/*.ts\""
  },
